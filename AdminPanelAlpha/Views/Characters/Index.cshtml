@{
    ViewData["Title"] = "Home Page";
    IEnumerable<Character> characters = ViewData.Model;
}

<div class="text-center">
    <h1 class="display-4">Characters Data</h1>
    
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
        @if (@characters != null)
            @foreach (var @c in @characters) 
            {
                <tr>
                    <td>@c.Id</td>
                    <td>@c.Name</td>
                    <td>@c.Price</td>
                </tr>
            }
    </table>
</div>
